<div class="span12"><h1>Edit: <%= @picture[:title] %>, <%= @picture[:artist] %></h1>  </div>
<p>
  <%= link_to "Back", @picture, :class => "btn btn-inverse"%>
</p>
<div class="row-fluid">
      <div class="span6" style="margin-left: 50px;">
      <%= form_for @picture do |f| %>
        <p>
          <%= f.label "artist" %><br>
          <%= f.text_field "artist" %>
        </p>
        <p>
          <%= f.label "title" %><br>
          <%= f.text_field "title" %>
        </p>
        <p>
          <%= f.label "url" %><br>
          <%= f.text_field "url" %>
        </p>
        <p>
          <%= submit_tag "Save", :class => "btn btn-warning" %>
        </p>
      <% end %>
    </div>
  </div>